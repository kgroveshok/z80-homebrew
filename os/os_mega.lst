73c5			; Initialise alphanumeric LCD module  
73c5			; LCD control register codes:  
73c5			;   DL   0 = 4-bit mode        1 = 8-bit mode  
73c5			;   N    0 = 1-line mode       1 = 2-line mode  
73c5			;   F    0 = Font 5 x 8        1 = Font 5 x 11  
73c5			;   D    0 = Display off       1 = Display on  
73c5			;   C    0 = Cursor off        1 = Cursor on  
73c5			;   B    0 = Blinking off      1 = Blinking on  
73c5			;   ID   0 = Decrement mode    1 = Increment mode  
73c5			;   SH   0 = Entire shift off  1 = Entire shift on  
73c5 3e 28		fLCD_Init:  LD   A, 40  
73c7 cd ec 74		            CALL LCDDelay       ;Delay 40ms after power up  
73ca			; For reliable reset set 8-bit mode - 3 times  
73ca cd bc 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73cd cd bc 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73d0 cd bc 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73d3			; Set 4-bit mode  
73d3 cd b8 74		            CALL WrFn4bit       ;Function = 4-bit mode  
73d6 cd ea 74		            CALL LCDDelay1      ;Delay 37 us or more  
73d9			; Function set  
73d9 3e 28		            LD   A, 00101000b  ;Control reg:  0  0  1  DL N  F  x  x  
73db cd ee 73		            CALL fLCD_Inst      ;2 line, display on  
73de			; Display On/Off control  
73de 3e 0c		            LD   A, 00001100b  ;Control reg:  0  0  0  0  1  D  C  B   
73e0 cd ee 73		            CALL fLCD_Inst      ;Display on, cursor on, blink off  
73e3			; Display Clear  
73e3 3e 01		            LD   A, 00000001b  ;Control reg:  0  0  0  0  0  0  0  1  
73e5 cd ee 73		            CALL fLCD_Inst      ;Clear display  
73e8			; Entry mode  
73e8 3e 06		            LD   A, 00000110b  ;Control reg:  0  0  0  0  0  1  ID SH  
73ea cd ee 73		            CALL fLCD_Inst      ;Increment mode, shift off  
73ed			; Display module now initialised  
73ed c9			            RET  
73ee			; ok to here  
