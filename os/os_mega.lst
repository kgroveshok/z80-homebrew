7350			; **  Constants  
7350			; **********************************************************************  
7350			; LCD constants required by LCD support module  
7350			kLCDPrt:    EQU kDataReg       ;LCD port is the PIO port A data reg  
7350			kLCDBitRS:  EQU 2              ;Port bit for LCD RS signal  
7350			kLCDBitE:   EQU 3              ;Port bit for LCD E signal             
7350			kLCDBitE2:   EQU 0              ;Port bit for LCD E2 signal            VIDOUT  
7350			; TODO Decide which E is being set  
7350			kLCDWidth:  EQU display_cols             ;Width in characters  
