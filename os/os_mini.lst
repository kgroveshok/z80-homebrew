73f5			; **********************************************************************  
73f5			; **  Alphanumeric LCD support                  by Stephen C Cousins  **  
73f5			; **********************************************************************  
73f5			;  
73f5			; **  Written as a Small Computer Monitor App   
73f5			; **  Version 0.1 SCC 2018-05-16  
73f5			; **  www.scc.me.uk  
73f5			;  
73f5			; **********************************************************************  
73f5			;  
73f5			; This module provides support for alphanumeric LCD modules using with  
73f5			; *  HD44780 (or compatible) controller  
73f5			; *  5 x 7 pixel fonts  
73f5			; *  Up to 80 characters in total (eg. 4 lines of 20 characters)  
73f5			; *  Interface via six digital outputs to the display (see below)  
73f5			;  
73f5			; LCD module pinout:  
73f5			;   1  Vss   0v supply  
73f5			;   2  Vdd   5v supply  
73f5			;   3  Vo    LCD input voltage (near zero volts via potentiometer)  
73f5			;   4  RS    High = data, Low = instruction  
73f5			;   5  R/W   High = Read, Low = Write  
73f5			;   6  E     Enable signal (active high)  
73f5			;   7  DB0   Data bit 0  
73f5			;   8  DB1   Data bit 1  
73f5			;   9  DB2   Data bit 2  
73f5			;  10  DB3   Data bit 3  
73f5			;  11  DB4   Data bit 4  
73f5			;  12  DB5   Data bit 5  
73f5			;  13  DB6   Data bit 6  
73f5			;  14  DB7   Data bit 7  
73f5			;  15  A     Backlight anode (+)  
73f5			;  16  K     Backlight cathode (-)  
73f5			;  
73f5			; This interfacing method uses 4-bit data mode and uses time delays  
73f5			; rather than polling the display's ready status. As a result the   
73f5			; interface only requires 6 simple output lines:  
73f5			;   LCD E   = Microcomputer output port bit <kLCDBitE>  
73f5			;   LCD RS  = Microcomputer output port bit <kLCDBitRS>  
73f5			;   LCD DB4 = Microcomputer output port bit 4  
73f5			;   LCD DB5 = Microcomputer output port bit 5  
73f5			;   LCD DB6 = Microcomputer output port bit 6  
73f5			;   LCD DB7 = Microcomputer output port bit 7  
73f5			; Display's R/W is connected to 0v so it is always in write mode  
73f5			; All 6 connections must be on the same port address <kLCDPrt>  
73f5			; This method also allows a decent length of cable from micro to LCD  
73f5			;  
73f5			; **********************************************************************  
73f5			;  
73f5			; To include the code for any given function provided by this module,   
73f5			; add the appropriate #REQUIRES <FunctionName> statement at the top of   
73f5			; the parent source file.  
73f5			; For example:  #REQUIRES   uHexPrefix  
73f5			;  
73f5			; Also #INCLUDE this file at some point after the #REQUIRES statements  
73f5			; in the parent source file.  
73f5			; For example:  #INCLUDE    ..\_CodeLibrary\Utilities.asm  
73f5			;  
73f5			; These are the function names provided by this module:  
73f5			; fLCD_Init                     ;Initialise LCD  
73f5			; fLCD_Inst                     ;Send instruction to LCD  
73f5			; fLCD_Data                     ;Send data byte to LCD  
73f5			; fLCD_Pos                      ;Position cursor  
73f5			; fLCD_Str                      ;Display string  
73f5			; fLCD_Def                      ;Define custom character  
73f5			;  
73f5			; **********************************************************************  
73f5			;  
73f5			; Requires SCMonAPI.asm to also be included in the project  
73f5			;  
73f5			  
73f5			  
73f5			; **********************************************************************  
73f5			; **  Constants  
73f5			; **********************************************************************  
73f5			  
73f5			; Constants that must be defined externally  
73f5			;kLCDPrt:   EQU 0xc0           ;Port address used for LCD  
73f5			;kLCDBitRS: EQU 2              ;Port bit for LCD RS signal  
73f5			;kLCDBitE:  EQU 3              ;Port bit for LCD E signal  
73f5			;kLCDWidth: EQU 20             ;Width in characters  
73f5			  
73f5			; general line offsets in any frame buffer  
73f5			  
73f5			  
73f5			display_row_1: equ 0  
73f5			display_row_2: equ display_row_1+display_cols  
73f5			display_row_3: equ display_row_2 + display_cols  
73f5			display_row_4: equ display_row_3 + display_cols  
73f5			;display_row_4_eol:   
73f5			  
73f5			  
73f5			; Cursor position values for the start of each line  
73f5			kLCD_Line1: EQU 0x00   
73f5			kLCD_Line2: EQU 0x40    
73f5			kLCD_Line3: EQU kLCD_Line1+kLCDWidth  
73f5			kLCD_Line4: EQU kLCD_Line2+kLCDWidth   
73f5			  
73f5			; Instructions to send as A register to fLCD_Inst  
73f5			kLCD_Clear: EQU 00000001b     ;LCD clear  
73f5			kLCD_Off:   EQU 00001000b     ;LCD off  
73f5			kLCD_On:    EQU 00001100b     ;LCD on, no cursor or blink  
73f5			kLCD_Under: EQU 00001110b     ;LCD on, cursor = underscore  
73f5			kLCD_Blink: EQU 00001101b     ;LCD on, cursor = blink block  
73f5			kLCD_Both:  EQU 00001111b     ;LCD on, cursor = under+blink  
73f5			  
73f5			; Constants used by this code module  
73f5			kLCD_Clr:   EQU 00000001b     ;LCD command: Clear display  
73f5			kLCD_Pos:   EQU 10000000b     ;LCD command: Position cursor  
73f5			kLCD_Def:   EQU 01000000b     ;LCD command: Define character  
73f5			  
