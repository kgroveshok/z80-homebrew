15a9			      ; split a bigger block into two - requested size and remaining size 
15a9			malloc_alloc_split: 
15a9			      ex    DE, HL                  ; Calculate address of new free block 
15a9			      dec   HL 
15a9			      dec   HL 
15a9			      dec   HL 
