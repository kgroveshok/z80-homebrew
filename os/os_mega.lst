73de			; Initialise alphanumeric LCD module  
73de			; LCD control register codes:  
73de			;   DL   0 = 4-bit mode        1 = 8-bit mode  
73de			;   N    0 = 1-line mode       1 = 2-line mode  
73de			;   F    0 = Font 5 x 8        1 = Font 5 x 11  
73de			;   D    0 = Display off       1 = Display on  
73de			;   C    0 = Cursor off        1 = Cursor on  
73de			;   B    0 = Blinking off      1 = Blinking on  
73de			;   ID   0 = Decrement mode    1 = Increment mode  
73de			;   SH   0 = Entire shift off  1 = Entire shift on  
73de 3e 28		fLCD_Init:  LD   A, 40  
73e0 cd 05 75		            CALL LCDDelay       ;Delay 40ms after power up  
73e3			; For reliable reset set 8-bit mode - 3 times  
73e3 cd d5 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73e6 cd d5 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73e9 cd d5 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73ec			; Set 4-bit mode  
73ec cd d1 74		            CALL WrFn4bit       ;Function = 4-bit mode  
73ef cd 03 75		            CALL LCDDelay1      ;Delay 37 us or more  
73f2			; Function set  
73f2 3e 28		            LD   A, 00101000b  ;Control reg:  0  0  1  DL N  F  x  x  
73f4 cd 07 74		            CALL fLCD_Inst      ;2 line, display on  
73f7			; Display On/Off control  
73f7 3e 0c		            LD   A, 00001100b  ;Control reg:  0  0  0  0  1  D  C  B   
73f9 cd 07 74		            CALL fLCD_Inst      ;Display on, cursor on, blink off  
73fc			; Display Clear  
73fc 3e 01		            LD   A, 00000001b  ;Control reg:  0  0  0  0  0  0  0  1  
73fe cd 07 74		            CALL fLCD_Inst      ;Clear display  
7401			; Entry mode  
7401 3e 06		            LD   A, 00000110b  ;Control reg:  0  0  0  0  0  1  ID SH  
7403 cd 07 74		            CALL fLCD_Inst      ;Increment mode, shift off  
7406			; Display module now initialised  
7406 c9			            RET  
7407			; ok to here  
