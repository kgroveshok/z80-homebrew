73f5			; **********************************************************************  
73f5			; **  LCD support functions  
73f5			; **********************************************************************  
73f5			  
73f5			; Initialise alphanumeric LCD module  
73f5			; LCD control register codes:  
73f5			;   DL   0 = 4-bit mode        1 = 8-bit mode  
73f5			;   N    0 = 1-line mode       1 = 2-line mode  
73f5			;   F    0 = Font 5 x 8        1 = Font 5 x 11  
73f5			;   D    0 = Display off       1 = Display on  
73f5			;   C    0 = Cursor off        1 = Cursor on  
73f5			;   B    0 = Blinking off      1 = Blinking on  
73f5			;   ID   0 = Decrement mode    1 = Increment mode  
73f5			;   SH   0 = Entire shift off  1 = Entire shift on  
73f5 3e 28		fLCD_Init:  LD   A, 40  
73f7 cd ac 74		            CALL LCDDelay       ;Delay 40ms after power up  
73fa			; For reliable reset set 8-bit mode - 3 times  
73fa cd 96 74		            CALL WrFn8bit       ;Function = 8-bit mode  
73fd cd 96 74		            CALL WrFn8bit       ;Function = 8-bit mode  
7400 cd 96 74		            CALL WrFn8bit       ;Function = 8-bit mode  
7403			; Set 4-bit mode  
7403 cd 92 74		            CALL WrFn4bit       ;Function = 4-bit mode  
7406 cd aa 74		            CALL LCDDelay1      ;Delay 37 us or more  
7409			; Function set  
7409 3e 28		            LD   A, 00101000b  ;Control reg:  0  0  1  DL N  F  x  x  
740b cd 1e 74		            CALL fLCD_Inst      ;2 line, display on  
740e			; Display On/Off control  
740e 3e 0c		            LD   A, 00001100b  ;Control reg:  0  0  0  0  1  D  C  B   
7410 cd 1e 74		            CALL fLCD_Inst      ;Display on, cursor on, blink off  
7413			; Display Clear  
7413 3e 01		            LD   A, 00000001b  ;Control reg:  0  0  0  0  0  0  0  1  
7415 cd 1e 74		            CALL fLCD_Inst      ;Clear display  
7418			; Entry mode  
7418 3e 06		            LD   A, 00000110b  ;Control reg:  0  0  0  0  0  1  ID SH  
741a cd 1e 74		            CALL fLCD_Inst      ;Increment mode, shift off  
741d			; Display module now initialised  
741d c9			            RET  
741e			; ok to here  
741e			  
741e			; Write instruction to LCD  
741e			;   On entry: A = Instruction byte to be written  
741e			;   On exit:  AF BC DE HL IX IY I AF' BC' DE' HL' preserved  
741e f5			fLCD_Inst:  PUSH AF  
741f f5			            PUSH AF  
7420 cd 32 74		            CALL Wr4bits       ;Write bits 4 to 7 of instruction  
7423 f1			            POP  AF  
7424 17			            RLA                 ;Rotate bits 0-3 into bits 4-7...  
7425 17			            RLA  
7426 17			            RLA  
7427 17			            RLA  
7428 cd 32 74		            CALL Wr4bits       ;Write bits 0 to 3 of instruction  
742b 3e 02		            LD   A, 2  
742d cd ac 74		            CALL LCDDelay       ;Delay 2 ms to complete   
7430 f1			            POP  AF  
7431 c9			            RET  
7432 e6 f0		Wr4bits:   AND  0xF0           ;Mask so we only have D4 to D7  
7434 d3 c0		            OUT  (kLCDPrt), A   ;Output with E=Low and RS=Low  
7436 cb df		            SET  kLCDBitE, A  
7438 d3 c0		            OUT  (kLCDPrt), A   ;Output with E=High and RS=Low  
743a cb 9f		            RES  kLCDBitE, A  
743c d3 c0		            OUT  (kLCDPrt), A   ;Output with E=Low and RS=Low  
743e c9			            RET  
743f			  
743f			  
743f			; Write data to LCD  
743f			;   On entry: A = Data byte to be written  
743f			;   On exit:  AF BC DE HL IX IY I AF' BC' DE' HL' preserved  
743f f5			fLCD_Data:  PUSH AF  
7440 f5			            PUSH AF  
7441 cd 53 74		            CALL Wr4bitsa       ;Write bits 4 to 7 of data byte  
7444 f1			            POP  AF  
7445 17			            RLA                 ;Rotate bits 0-3 into bits 4-7...  
7446 17			            RLA  
7447 17			            RLA  
7448 17			            RLA  
7449 cd 53 74		            CALL Wr4bitsa       ;Write bits 0 to 3 of data byte  
744c 3e 96		            LD   A, 150  
744e 3d			Wait:      DEC  A              ;Wait a while to allow data   
744f 20 fd		            JR   NZ, Wait      ;  write to complete  
7451 f1			            POP  AF  
7452 c9			            RET  
7453 e6 f0		Wr4bitsa:   AND  0xF0           ;Mask so we only have D4 to D7  
7455 cb d7		            SET  kLCDBitRS, A  
7457 d3 c0		            OUT  (kLCDPrt), A   ;Output with E=Low and RS=High  
7459 cb df		            SET  kLCDBitE, A  
745b d3 c0		            OUT  (kLCDPrt), A   ;Output with E=High and RS=High  
745d cb 9f		            RES  kLCDBitE, A  
745f d3 c0		            OUT  (kLCDPrt), A   ;Output with E=Low and RS=High  
7461 cb 97		            RES  kLCDBitRS, A  
7463 d3 c0		            OUT  (kLCDPrt), A   ;Output with E=Low and RS=Low  
7465 c9			            RET  
7466			  
7466			  
7466			; Position cursor to specified location  
7466			;   On entry: A = Cursor position  
7466			;   On exit:  AF BC DE HL IX IY I AF' BC' DE' HL' preserved  
7466 f5			fLCD_Pos:   PUSH AF  
7467 f6 80		            OR   kLCD_Pos       ;Prepare position cursor instruction  
7469 cd 1e 74		            CALL fLCD_Inst      ;Write instruction to LCD  
746c f1			            POP  AF  
746d c9			            RET  
